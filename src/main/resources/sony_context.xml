<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-lazy-init="true">

    <bean class="impls.sony.SonyHand">
        <property name="color" value="golden"/>
        <qualifier value="sonyGoldenHand"/>
    </bean>
    <bean class="impls.sony.SonyHand">
        <property name="color" value="bronze"/>
        <qualifier value="sonyBronzeHand"/>
    </bean>
    <bean id="sonyHead" class="impls.sony.SonyHead"/>
    <bean id="sonyLeg" class="impls.sony.SonyLeg"/>

</beans>